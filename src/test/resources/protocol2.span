# Example Protocol

.metadata

    version : 1.0;
    recipe size : 1;

.constants

    fraction p : 1/2;

.signature

    variables : m, r, k, u, v;
    functions : enc/3, dec/2, h/1;
    public names : a, b, c;
    private names : x, y, z;

.rewrites

    rewrite : dec(enc(m,r,k), k) -> m;

.roles

    role : [T] out(p -> h(a) + p -> x);

.safety
    secrecy : x >= 1/2;